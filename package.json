	"main": "index.jsx",
	"scripts": {
		"test": "test",
		"prestart": "babel-node --presets node6 server/startMessage.js",
		"start": "npm-run-all --parallel open:client lint:watch",
		"open:client": "babel-node --presets node6 server/srcServer.js",
		"lint": "node_modules/.bin/esw webpack.config.* client server",
		"lint:watch": "npm run lint -- --watch",
