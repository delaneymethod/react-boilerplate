		"build:html": "babel-node --presets node6 server/buildHtml.js",
		"prebuild": "npm-run-all clean-public lint build:html",
		"build": "babel-node --presets node6 server/build.js",
		"postbuild": "babel-node --presets node6 server/publicServer.js"
	},
